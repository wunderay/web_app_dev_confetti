<h2 class="center"><%= course.title %> Course Information</h2>
<table class="table">
    <thead>
        <tr>
            <th>Course</th>
            <th>Description</th>
            <th>Number of Students</th>
            <th>Cost $</th>
        </tr>
    </thead>
    <tbody>
            <tr>
                <td>
                    <%= course.title %>
                </td>

                <td>
                    <%= course.description %>
                </td>
                <td>
                    <%= course.maxStudents %>
                </td>
                <td>
                    <%= course.cost %>
                </td>
            </tr>
    </tbody>
</table>
</table>